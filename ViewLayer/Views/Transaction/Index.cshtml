@model IEnumerable<E_Food_Admin.Models.TransactionApiModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TransactionId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PaymentMethodId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AccountId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Value)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TimeTrans)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsSuccess)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Note)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.TransactionId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PaymentMethodId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AccountId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Value)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TimeTrans)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IsSuccess)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Note)
                </td>
            <td>
                    @Html.ActionLink("Delete", "Delete", new {  id=item.TransactionId })
                    @Html.ActionLink("Confirm", "Confirm", new {  id=item.TransactionId })
            </td>
        </tr>
}
    </tbody>
</table>
